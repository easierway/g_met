<seelog>
    <outputs>
        <console formatid="console"/>
        <conn formatid="syslog" net="tcp" addr="localhost:514" tls="false" />
    </outputs>
    <formats>
        <format id="console" format="%Date %Time [%LEVEL] %Msg%n"/>
        <format id="syslog" format="[%LEVEL] %Msg%n"/>
    </formats>
</seelog>
